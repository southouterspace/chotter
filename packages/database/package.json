{
  "name": "@chotter/database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./auth": "./src/auth.ts",
    "./queries": "./src/queries/index.ts",
    "./types": "./src/types/database.ts"
  },
